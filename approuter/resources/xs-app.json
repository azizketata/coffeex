{
  "welcomeFile": "/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/view/(.*\\.css)$",
      "target": "$1",
      "localDir": "webapp/view",
      "authenticationType": "none"
    },
    {
      "source": "^/coffee-frontend/view/(.*\\.css)$",
      "target": "$1",
      "localDir": "webapp/view",
      "authenticationType": "none"
    },
    {
      "source": "^/odata/(.*)$",
      "target": "/odata/$1",
      "destination": "coffeex-srv-api",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/api/(.*)$",
      "target": "/api/$1",
      "destination": "coffeex-srv-api",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/srv/(.*)$",
      "target": "/srv/$1",
      "destination": "coffeex-srv-api",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^(.*)$",
      "target": "$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "xsuaa"
    }
  ]
}
