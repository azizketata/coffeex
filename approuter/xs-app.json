{
  "welcomeFile": "/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/user-api/currentUser$", 
      "target": "/currentUser",
      "service": "sap-approuter-userapi",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/odata/(.*)$",
      "target": "/odata/$1",
      "destination": "coffeex-srv-api",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/api/(.*)$",
      "target": "/api/$1",
      "destination": "coffeex-srv-api",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/srv/(.*)$",
      "target": "/srv/$1",
      "destination": "coffeex-srv-api",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^(.*)$",
      "target": "$1",
      "localDir": "resources",
      "authenticationType": "xsuaa"
    }
  ]
}
