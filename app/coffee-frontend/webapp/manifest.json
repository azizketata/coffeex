{
  "_version": "1.12.0",
  "sap.app": {
    "id": "coffee-frontend",
    "type": "application",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "Coffee Frontend App",
    "description": "Login-based UI5 app",
    "sourceTemplate": {
      "id": "@sap/ux-app-migrator:freestyle",
      "version": "1.18.1"
    },
    "dataSources": {
      "coffeeService": {
        "uri": "/odata/v4/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "coffee-display": {
          "semanticObject": "coffee",
          "action": "display",
          "title": "Coffee Frontend App",
          "signature": {
            "parameters": {}
          },
          "resolutionResult": {
            "applicationType": "SAPUI5",
            "additionalInformation": "SAPUI5.Component=coffee-frontend",
            "url": "./"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "URL"
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "coffee-frontend.view.Main",
      "type": "XML",
      "id": "main"
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "coffee-frontend.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "async": true,
        "bypassed": {
          "target": "notFound"
        }
      },
      "routes": [
        {
          "pattern": "",
          "name": "homeDefault",
          "target": "home"
        },
        {
          "pattern": "order-success",
          "name": "orderSuccess",
          "target": "orderSuccess"
        },
        {
          "pattern": "privacy",
          "name": "privacy",
          "target": "privacy"
        },
        {
          "pattern": "home",
          "name": "home",
          "target": "home"
        },
        {
          "pattern": "orders",
          "name": "orders",
          "target": "orders"
        },
        {
          "pattern": "profile",
          "name": "profile",
          "target": "profile"
        },
        {
          "pattern": "history",
          "name": "history",
          "target": "history"
        },
        {
          "pattern": "admin",
          "name": "admin",
          "target": "admin"
        },
        
        {
          "pattern": ":all*:",
          "name": "notFound",
          "target": "notFound"
        }
      ],
      "targets": {
        "home": {
          "viewName": "Home",
          "viewId": "home",
          "viewLevel": 1
        },
        "orderSuccess": {
          "viewName": "OrderSuccess",
          "viewId": "orderSuccess",
          "viewLevel": 5
        },
        "privacy": {
          "viewName": "Privacy",
          "viewId": "privacy",
          "viewLevel": 5
        },
        "orders": {
          "viewName": "Orders",
          "viewId": "orders",
          "viewLevel": 2
        },
        "profile": {
          "viewName": "Profile",
          "viewId": "profile",
          "viewLevel": 3
        },
        "history": {
          "viewName": "History",
          "viewId": "history",
          "viewLevel": 4
        },
        "admin": {
          "viewName": "Admin",
          "viewId": "admin",
          "viewLevel": 6
        },

        "notFound": {
          "viewName": "NotFound",
          "viewId": "notFound",
          "transition": "show"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/Home.view.css"
        },
        {
          "uri": "css/History.view.css"
        },
        {
          "uri": "css/Profile.view.css"
        },
        {
          "uri": "css/OrderSuccess.view.css"
        },
        {
          "uri": "css/Privacy.view.css"
        },
        {
          "uri": "css/CheckoutDialog.css"
        },
        {
          "uri": "css/TopUpDialog.css"
        }
      ]
    },
    "models": {
      "": {
        "dataSource": "coffeeService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true
        },
        "type": "sap.ui.model.odata.v4.ODataModel"
      }
    }
  },
  "sap.cloud": {
    "service": "coffee"
  }
}
